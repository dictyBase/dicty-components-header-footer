{
  "name": "dicty-components-header-footer",
  "version": "0.0.0-semantically-released",
  "description": "React component for header and footer of dictyBase web applications",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --no-demo --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest",
    "test:watch": "jest --watch",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/es/",
      "<rootDir>/lib/"
    ],
    "collectCoverageFrom": [
      "src/components/*.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/es/",
      "!<rootDir>/lib/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "flow": true,
          "typescript": false
        }
      ],
      [
        "@babel/env",
        {
          "modules": "false",
          "useBuiltIns": "usage"
        }
      ],
      "@babel/react"
    ]
  },
  "dependencies": {
    "@material-ui/icons": "^4.9.1"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.10.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@material-ui/core": "^4.10.1",
    "@semantic-release/git": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-react-app": "^9.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.5.0",
    "flow-bin": "^0.123.0",
    "flow-typed": "^3.1.0",
    "font-awesome": "^4.7.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jss": "^10.1.1",
    "lint-staged": "^10.1.6",
    "nwb": "^0.24.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-styleguidist": "^11.0.5",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.0.6",
    "sinon": "^9.0.2",
    "typeface-roboto": "0.0.75"
  },
  "author": "",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/dicty-components-header-footer.git"
  },
  "keywords": [
    "react-component"
  ]
}
